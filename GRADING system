let mark = 50
if (mark>=80 && mark <= 100){
    console.log ("Grade A ")
}
else if (mark>=60 && mark <=79){
    console.log ("Grade B")
}
else if (mark>=49 && mark <=59){
    console.log ("Grade C")
}
else if (mark>=40 && mark <=49){
    console.log ("Grade D")
}
else if (mark<=40){
    console.log ("Grade E")
}